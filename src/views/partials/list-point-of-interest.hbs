<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
    </tr>
  </thead>
  <tbody>
    {{#each pois}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{description}}
        </td>
        <td>
          Latitude:
          {{location.lat}}, Longitude:
          {{location.lng}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
